import java.util.Scanner; public class Lucky {    public static void main(String[] args) {        Scanner sc=new Scanner(System.in);        int t=sc.nextInt();         while(t-->0){            String s=sc.next();            int fSum=0;            int lsum=0;            for (int i=0;i<s.length();i++) {                int val = (int) s.charAt(i) - '0';                if (i < 3) fSum += val;                else lsum += val;            }             if(lsum==fSum) System.out.println("YES");            else System.out.println("NO");        }    }}